-router-dom\";\nimport Login from './Login'\n\n\n\nconst Signup = () => {\n    const [username, setUsername] = useState('')\n    const [password, setPassword] = useState('')\n    const history = useHistory()\n\n    const goHome = () => {\n        history.push('/')\n    }\n\n    const signup = async () => {\n        try {\n            await axios.post('/account/signup', { username, password })\n            goHome()\n        } catch {\n            window.alert('could not sign up')\n        }\n    }\n      \n    return (\n        <div>\n        Username: <input onChange={e => setUsername(e.target.value)} />\n        Password: <input onChange={e => setPassword(e.target.value)} />\n        <br></br>\n        <button onClick={() => signup(username, password)}> register user </button>\n        <br></br>\n        <div className=\"form-group\">\n            Already have an account?<Link to=\"/login\"> Login Here </Link>\n        </div>        \n        <Switch>\n            <Route path=\"/login\">\n                <Login />\n            </Route>\n        </Switch>\n        </div>\n    )\n}\n\nexport default Signup"},"lineCount":null}},"error":null,"hash":"fdaefb604c8dc3f8c8847b5ac4eacbf9","cacheData":{"env":{}}}